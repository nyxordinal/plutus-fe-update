    "@reduxjs/toolkit": "^1.9.0",
    "@tailwindcss/forms": "0.2.1",
    "axios": "^1.1.3",
    "chart.js": "2.9.4",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "js-cookie": "^3.0.1",
    "next": "10.0.9",
    "node-rsa": "^1.1.1",
    "prop-types": "^15.8.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^8.0.5",
